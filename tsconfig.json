{
	"compilerOptions": {
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [
			"ES6"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,
		"noUnusedLocals": true,
		"pretty": true,
		"removeComments": true,
		"sourceMap": false,
		"target": "es6"
	}
}